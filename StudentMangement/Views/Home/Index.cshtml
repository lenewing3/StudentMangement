@model IEnumerable<Student>
@{ 
    ViewBag.Title = "学生列表页";
}
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>名字</th>
            <th>年级</th>
            <th>邮箱</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>
                    @student.Id
                </td>
                <td>
                    @student.Name
                </td>
                <td>
                    @student.ClassName
                </td>
                <td>
                    @student.Email
                </td>
            </tr>
        }
    </tbody>
</table>